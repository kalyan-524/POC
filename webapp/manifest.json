{
  "_version": "1.65.0",
  "sap.app": {
    "id": "ladera.royalenfield",
    "type": "application",
    "i18n": "i18n/i18n.properties",
    "applicationVersion": {
      "version": "0.0.1"
    },
    "title": "{{appTitle}}",
    "description": "{{appDescription}}",
    "resources": "resources.json",
    "sourceTemplate": {
      "id": "@sap/generator-fiori:basic",
      "version": "1.18.4",
      "toolsId": "06acc47e-67aa-4e72-927b-613b72b4cba3"
    }
  },
  "sap.ui": {
    "technology": "UI5",
    "icons": {
      "icon": "",
      "favIcon": "",
      "phone": "",
      "phone@2": "",
      "tablet": "",
      "tablet@2": ""
    },
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    }
  },
  "sap.ui5": {
    "flexEnabled": false,
    "dependencies": {
      "minUI5Version": "1.138.1",
      "libs": {
        "sap.m": {},
        "sap.ui.core": {}
      }
    },
    "contentDensities": {
      "compact": true,
      "cozy": true
    },
    "models": {
      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "settings": {
          "bundleName": "ladera.royalenfield.i18n.i18n"
        }
      },
      "credentials": { 

    "type": "sap.ui.model.json.JSONModel", 

    "uri": "model/credentials.json" 
    },
      "data": { 

    "type": "sap.ui.model.json.JSONModel", 

    "uri": "model/data.json" 
    }
     

  },
    "resources": {
      "css": [
        {
          "uri": "css/style.css"
        }
      ]
    },
    "routing": {
      "config": {
        "routerClass": "sap.m.routing.Router",
        "controlAggregation": "pages",
        "controlId": "appviewid",
        "bypassed": {
          "target" : "RouteView1" 
        },
        "transition": "slide",
        "type": "View",
        "viewType": "XML",
        "path": "ladera.royalenfield.view",
        "async": true,
        "viewPath": "ladera.royalenfield.view"
      },
      "routes": [
        {
          "name": "RouteView1",
          "pattern": ":?query:",
          "target": [
            "TargetView1"
          ]
        },
        {
          "name": "homescreen",
          "pattern": "homescreen",
          "target": [
            "homescreen"
          ]
        },
        {
          "name": "purchaseorder",
          "pattern": "purchaseorder",
          "target": [
            "thirdview"
          ]
        },
        {
          "name": "catalog",
          "pattern": "catalog",
          "target": [
            "catalogview"
          ]
        },
        {
          "name": "sofatile",
          "pattern": "sofatile",
          "target": [
            "sofatileview"
          ]
        }
      ],
      
      "targets": {
        "TargetView1": {
          "id": "View1",
          "name": "View1"
        },
      "homescreen":{
          "id": "homescreen",
          "name": "homescreen"
        },
        "thirdview":{
          "id": "po",
          "name": "purchaseorder"
        },
        "catalogview":{
          "id": "catalog",
          "name": "catalog"
        },
        "sofatileview":{
          "id": "sofatile",
          "name": "sofatile"
        }
      }
    },
    "rootView": {
      "viewName": "ladera.royalenfield.view.App",
      "type": "XML",
      "id": "App",
      "async": true
    }
  }
}
